{
  "name": "@constructor-io/constructorio-ui-autocomplete",
  "version": "1.2.0",
  "description": "Constructor.io Autocomplete UI library for web applications",
  "main": "lib/cjs/index.js",
  "module": "lib/mjs/index.js",
  "exports": {
    ".": {
      "import": "./lib/mjs/index.js",
      "require": "./lib/cjs/index.js"
    }
  },
  "homepage": "https://github.com/Constructor-io/constructorio-ui-autocomplete#readme",
  "types": "lib/types/index.d.ts",
  "scripts": {
    "lint": "eslint src --ext js,ts,jsx,tsx",
    "copy-files": "copyfiles -u 1 src/**/*.css lib/cjs && copyfiles -u 1 src/**/*.css lib/mjs",
    "prepare": "husky install",
    "dev": "storybook dev -p 6006",
    "storybook:ci": "storybook dev --ci --quiet -p 6006",
    "test-storybook": "test-storybook",
    "test-storybook:watch": "test-storybook --watch",
    "test-storybook:ci": "start-server-and-test storybook:ci http://localhost:6006 test-storybook",
    "build-storybook": "storybook build -o 'docs'",
    "serve-built-storybook": "npx http-server docs",
    "version": "npm run build-storybook && git add -u ./docs && git add ./docs/* && npm run compile",
    "compile": "rm -rf lib && tsc -p tsconfig.json && tsc -p tsconfig-cjs.json && npm run copy-files"
  },
  "author": "constructor.io",
  "license": "MIT",
  "volta": {
    "node": "16.17.0",
    "npm": "8.19.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Constructor-io/constructorio-ui-autocomplete.git"
  },
  "bugs": {
    "url": "https://github.com/Constructor-io/constructorio-ui-autocomplete/issues"
  },
  "files": [
    "lib/**/*"
  ],
  "devDependencies": {
    "@constructor-io/constructorio-client-javascript": "^2.34.1",
    "@cspell/eslint-plugin": "^6.18.1",
    "@storybook/addon-a11y": "^7.0.0-beta.40",
    "@storybook/addon-actions": "^7.0.0-beta.40",
    "@storybook/addon-essentials": "^7.0.0-beta.40",
    "@storybook/addon-interactions": "^7.0.0-beta.40",
    "@storybook/addon-links": "^7.0.0-beta.40",
    "@storybook/jest": "^0.0.11-next.0",
    "@storybook/react-webpack5": "7.0.0-beta.40",
    "@storybook/test-runner": "^0.10.0-next.8",
    "@storybook/testing-library": "^0.0.14-next.1",
    "@types/react": "^18.0.20",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.38.0",
    "@typescript-eslint/parser": "^5.38.0",
    "copyfiles": "^2.4.1",
    "downshift": "^7.0.5",
    "eslint": "^8.23.1",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.6.8",
    "husky": "^8.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "start-server-and-test": "^1.15.2",
    "storybook": "^7.0.0-beta.40",
    "typescript": "^4.8.3",
    "webpack": "^5.75.0"
  },
  "peerDependencies": {
    "@constructor-io/constructorio-client-javascript": "^2.29.11",
    "downshift": "^6.1.11",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tslib": "^2.4.0"
  }
}
